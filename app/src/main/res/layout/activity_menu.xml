<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MenuActivity">

    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:padding="5dp"
        android:elevation="5dp"
        android:layout_height="50dp"
        app:title="HOME ADMIN"
        app:titleTextColor="@color/blacksemu"
        android:background="@color/bg1">
        <ImageView
            android:id="@+id/img_back_menu"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/ic_back2"
            android:layout_gravity="end"/>
    </androidx.appcompat.widget.Toolbar>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/blacksemu"/>

    <LinearLayout
        android:elevation="5dp"
        android:padding="5dp"
        android:background="@color/bg1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/img_storage"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/ic_storage"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="20dp"/>
        <TextView
            android:id="@+id/txt_data"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:textColor="@color/blacksemu"
            android:fontFamily="@font/poppinsmedium"
            android:textSize="15sp"
            android:text="Data "
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"/>
        <ImageView
            android:id="@+id/img_reward"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/ic_rewards"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="20dp"/>
        <TextView
            android:id="@+id/txt_reward"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:textColor="@color/blacksemu"
            android:fontFamily="@font/poppinsmedium"
            android:textSize="15sp"
            android:text="Form Reward"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"/>
        <ImageView
            android:id="@+id/img_report"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/ic_reports"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="20dp"/>

        <TextView
            android:id="@+id/txt_report"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/poppinsmedium"
            android:text="Report "
            android:textColor="@color/blacksemu"
            android:textSize="15sp" />

    </LinearLayout>

    <FrameLayout
        android:id="@+id/frame_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>



</LinearLayout>